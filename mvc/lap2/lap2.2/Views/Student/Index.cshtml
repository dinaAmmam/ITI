@{
}
@model List<Student>

<html>
<head>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />



</head>
<body>
    <table class="table table-bordered table-hover ">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>

        </tr>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.id</td>
                <td>@student.name</td>
                <td>@student.age</td>

            </tr>
        }
    </table>
</body>
</html>